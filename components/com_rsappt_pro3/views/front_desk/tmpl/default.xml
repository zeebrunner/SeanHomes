			label="RS1_FD_RES_ADMIN_ONLY_LABEL" description="RS1_FD_RES_ADMIN_ONLY_HELP">
			<option value="Yes">RS1_FD_MENU_YES</option>
			<option value="No">RS1_FD_MENU_NO</option>
		</field>
		<field name="fd_login_required" type="radio"  default="Yes"
			label="RS1_FD_LOGIN_REQUIRED_LABEL" description="RS1_FD_LOGIN_REQUIRED_HELP">
			<option value="Yes">RS1_FD_MENU_YES</option>
			<option value="No">RS1_FD_MENU_NO</option>
		</field>
		<field name="fd_read_only" type="radio" default="No"
			label="RS1_FD_VIEW_ONLY_LABEL" description="RS1_FD_VIEW_ONLY_HELP">
			<option value="Yes">RS1_FD_MENU_YES</option>
			<option value="No">RS1_FD_MENU_NO</option>
		</field>
		<field name="fd_edit_status_only" type="radio"  default="No"
			label="RS1_FD_EDIT_STATUS_ONLY_LABEL" description="RS1_FD_EDIT_STATUS_ONLY_HELP">
			<option value="Yes">RS1_FD_MENU_YES</option>
			<option value="No">RS1_FD_MENU_NO</option>
		</field>
		<field name="fd_allow_show_seats" type="radio"  default="Yes"
			label="RS1_FD_ENABLE_SHOW_SEATS_LABEL" description="RS1_FD_ENABLE_SHOW_SEATS_HELP">
			<option value="Yes">RS1_FD_MENU_YES</option>
			<option value="No">RS1_FD_MENU_NO</option>
		</field>
		<field name="fd_allow_cust_hist" type="radio"  default="Yes"
			label="RS1_FD_ENABLE_CUST_HIST_LABEL" description="RS1_FD_ENABLE_CUST_HIST_HELP">
