			<option value="No">RS1_FD_MENU_NO</option>
		</field>
		<field name="fd_show_udfs" type="radio"  default="Yes"
			label="RS1_FD_SHOW_UDFS_LABEL" description="RS1_FD_SHOW_UDFS_HELP">
			<option value="Yes">RS1_FD_MENU_YES</option>
			<option value="No">RS1_FD_MENU_NO</option>
		</field>
		<field name="fd_show_extras" type="radio"  default="Yes"
			label="RS1_FD_SHOW_EXTRAS_LABEL" description="RS1_FD_SHOW_EXTRAS_HELP">
			<option value="Yes">RS1_FD_MENU_YES</option>
			<option value="No">RS1_FD_MENU_NO</option>
		</field>
		<field name="fd_show_financials" type="radio"  default="Yes"
			label="RS1_FD_SHOW_FINANCIALS_LABEL" description="RS1_FD_SHOW_FINANCIALS_HELP">
			<option value="Yes">RS1_FD_MENU_YES</option>
			<option value="No">RS1_FD_MENU_NO</option>
		</field>
		<field name="fd_show_cats_filter" type="radio"  default="No"
			label="RS1_FD_SHOW_CATS_LABEL" description="RS1_FD_SHOW_CATS_HELP">
			<option value="Yes">RS1_FD_MENU_YES</option>
			<option value="No">RS1_FD_MENU_NO</option>
		</field>
		<field name="fd_show_bookoffs" type="radio"  default="Yes"
			label="RS1_FD_SHOW_BO_LABEL" description="RS1_FD_SHOW_BO_HELP">
			<option value="Yes">RS1_FD_MENU_YES</option>
			<option value="No">RS1_FD_MENU_NO</option>
		</field>
		<field name="fd_booking_staff_or_public" type="list"  default="Staff"
			label="RS1_FD_NEW_BOOKING_LABEL" description="RS1_FD_NEW_BOOKING_HELP">
			<option value="Staff">RS1_FD_NEW_BOOKING_STAFF</option>
			<!-- removed, no longer works with SEO <option value="Public">RS1_FD_NEW_BOOKING_PUBLIC</option>-->
			<option value="None">RS1_FD_NEW_BOOKING_NONE</option>
		</field>
		
		</fieldset>
	</fields>
