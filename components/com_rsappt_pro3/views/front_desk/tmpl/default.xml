<?xml version="1.0" encoding="utf-8" ?> 
<metadata>
 	<layout title="ABPro Front Desk">
		<message>
			<![CDATA[COM_RSAPPT_PRO3_FRONT_DESK_VIEW_DEFAULT_TITLE]]>
		</message>
	</layout>

	<fields name="params">
		<fieldset name="params"	label="RS1_FD_BASIC_OPTIONS_FIELDSET_LABEL">
		
		<field name="fd_start_screen" type="list" default="month"
			label="RS1_FD_START_LABEL" description="RS1_FD_START_HELP">
			<option value="month">RS1_FD_START_MONTH</option>
			<option value="week">RS1_FD_START_WEEK</option>
			<option value="day">RS1_FD_START_DAY</option>
		</field>
		<field name="spacer" type="spacer" class="text" label="RS1_FD_MENU_SPACER" />		
		<field name="fd_use_page_title" type="radio"  default="No"
			label="RS1_FD_USE_PAGE_TITLE_LABEL" description="RS1_FD_USE_PAGE_TITLE_HELP">
			<option value="Yes">RS1_FD_MENU_YES</option>
			<option value="No">RS1_FD_MENU_NO</option>
		</field>
		<field name="fd_display" type="list"  default="Customer"
			label="RS1_FD_DISPLAY_LABEL" description="RS1_FD_DISPLAY_HELP">
			<option value="Customer">RS1_FD_DISPLAY_CUST</option>
			<option value="Resource">RS1_FD_DISPLAY_RES</option>
		</field>
		<field name="fd_tooltip" type="list"  default="Resource"
			label="RS1_FD_TOOLTIP_LABEL" description="RS1_FD_TOOLTIP_HELP">
			<option value="Customer">RS1_FD_TOOLTIP_CUST</option>
			<option value="Resource">RS1_FD_TOOLTIP_RES</option>
			<option value="None">RS1_FD_TOOLTIP_NONE</option>
		</field>
		<field name="spacer1" type="spacer" hr="true" />
